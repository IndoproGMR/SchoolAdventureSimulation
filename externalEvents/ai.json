      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": [
        "runtimeScene.setBackgroundColor(100,100,240);",
        "// var JumlahSiswa = runtimeScene.getVariables().get('siswa').getChild('JumlahSiswa').getAllChildrenArray().length;",
        "var JumlahSiswa = runtimeScene.getVariables().get('siswa').getChild('JumlahSiswa').getAsNumber();",
        "var variabler   = runtimeScene.getVariables().get('siswa').getChild('Siswa');",
        "var lvlsoal     = runtimeScene.getVariables().get('lvlsoal').getAsNumber();",
        "",
        "var ttotalScore  = runtimeScene.getVariables().get('TotalScore').getAsNumber();",
        "",
        "",
        "var score      = 0;",
        "var totaltotal = 0;",
        "",
        "for (let i = 0; i < JumlahSiswa; i++) {",
        "    score = 400 * lvlsoal; // Score awal yang akan di kalikan dengan level soal",
        "    ",
        "    score = score + 200; // score yang akan di tambahkan bila jawaban benar",
        "",
        "    var random_benar = Math.floor(Math.random() * 10) + 1 + lvlsoal;",
        "    var random_Kerajinan = Math.floor(Math.random() * 10) + 1 + lvlsoal;",
        "",
        "    var Kerajinan  = variabler.getChildAt(i).getChild('Kerajinan').getAsNumber() - lvlsoal;",
        "    var Kepintaran = variabler.getChildAt(i).getChild('Kepintaran').getAsNumber() - lvlsoal;",
        "    var Scoreyangada = variabler.getChildAt(i).getChild('ScoreSiswa').getAsNumber();",
        "",
        "",
        "    if(Kerajinan >= random_Kerajinan){",
        "        score = score + 100; // bonus score kalo bisa menjawaban soal dengan cepat",
        "    }",
        "    if(!Kepintaran >= random_benar){",
        "        score = 0;  // score di set ke 0 karena jawaban salah",
        "    }",
        "    var totalscore = score + Scoreyangada;",
        "",
        "    totaltotal = totalscore + totaltotal;",
        "",
        "    // simpan score",
        "    variabler.getChildAt(i).getChild('ScoreSiswa').setNumber(totalscore);",
        "}",
        "",
        "runtimeScene.getVariables().get('TotalScore').setNumber(totaltotal+ttotalScore);",
        "",
