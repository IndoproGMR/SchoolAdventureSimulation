{
  "associatedLayout": "Kelas",
  "lastChangeTimeStamp": 0,
  "name": "algonaiklvl",
  "events": [
    {
      "type": "BuiltinCommonInstructions::JsCode",
      "inlineCode": [
        "// belum di test",
        "//Get scene variables",
        "var variables = runtimeScene.getVariables();",
        "var JS = variables.get(\"JS\").getAsString(); // jumlah siswa",
        "var JN = variables.get(\"JN\").getAsString(); // jumlah nilai",
        "                                            // nilai tengah",
        "var NG = JN/2;",
        "var NR = variables.get(\"NR\").getAsString(); // nilai terenda",
        "var NT = variables.get(\"NT\").getAsString(); // nilai tertinggi",
        "                                            // nilai rata2",
        "var rataNilai = JN/JN;                      ",
        "var nilai = 0;",
        "var status=\"===\";",
        "",
        "//mulai",
        "function hitung(){",
        "    turun = turunlvl(rataNilai);",
        "    naik = naiklvl(rataNilai);",
        "    if(turun > naik){",
        "        status=\"---\";",
        "    }else if(turun < naik){",
        "        status=\"+++\";",
        "    }else if(turun == naik){",
        "        status=\"===\";",
        "    }",
        "    return status;",
        "}",
        "",
        "// rumus",
        "function turunlvl(rataNilai){",
        "    if(rataNilai <= NR){",
        "        nilai=1;",
        "    }else if(NR <= rataNilai && rataNilai <= NG){",
        "        nilai = (NG-rataNilai)/(NG-NR);",
        "    } else if(rataNilai>=NG){",
        "        nilai=0;",
        "    }",
        "    return nilai;",
        "}",
        "",
        "function naiklvl(rataNilai){",
        "        if(rataNilai <= NG){",
        "        nilai=0;",
        "    }else if(NG <= rataNilai && rataNilai <= NT){",
        "        nilai = (rataNilai-NG)/(NT-NG);",
        "    } else if(rataNilai>=NT){",
        "        nilai=1;",
        "    }",
        "    return nilai;",
        "}",
        "",
        "variables.get(\"test\").setString($nilai);"
      ],
      "parameterObjects": "",
      "useStrict": true,
      "eventsSheetExpanded": true
    }
  ]
}